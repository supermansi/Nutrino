	Hello ${userName}

	<form class="form-inline my-2 my-lg-0" action="recipeSearch" method="post">
		<input class="form-control mr-sm-2" name="recipeName" type="search" placeholder="Search"
			aria-label="Search" id="recipeName">
		<button class="btn btn-outline-success my-2 my-sm-0" type="submit">Search</button>
	</form>

	<table class="table">
		<thead class="thead-dark">
			<tr>
				<th>Name</th>
				<th>Ingredients</th>
				<th>Calories</th>
				<th>Weight</th>
				<th>Health Labels</th>
				<th>URL</th>
			</tr>
		</thead>
		<tbody>
			<c:forEach items="${recipes}" var="recipe">
				<tr>
					<td><c:out value="${recipe.getLabel()}" /></td>
					<td><c:out value="${recipe.getIngredient_lines()}" /></td>
					<td><c:out value="${recipe.getCalories()}" /></td>
					<td><c:out value="${recipe.getTotal_weight()}" /></td>
					<td><c:out value="${recipe.getHealth_labels()}" /></td>
					<td><c:out value="${recipe.getUrl()}" /></td>
				</tr>
			</c:forEach>
		</tbody>
	</table>
</body>
